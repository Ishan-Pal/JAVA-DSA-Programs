class Solution {
    public int differenceOfSum(int[] nums) {
        int x=0,d=0;
        for(int i=0;i<nums.length;i++)
        {
            x=x+nums[i];
            int n=nums[i];
            while(n>0)
            {
                d=d+n%10;
                n=n/10;

            }
            
         
        }
        return Math.abs(x-d);
        
        
        
    }
}
